{
    "name": "Stadia Enhanced",
    "description": "Various new features for Google Stadia.",
    "version": "1.19.1",
    "manifest_version": 3,
    "author": "Christopher Klay",
    "homepage_url": "https://github.com/ChristopherKlay/StadiaEnhanced",
    "icons": {
        "16": "media/logo/16.png",
        "32": "media/logo/32.png",
        "48": "media/logo/48.png",
        "128": "media/logo/128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Stadia Enhanced v1.19.1",
        "default_icon": {
            "16": "media/logo/16.png",
            "32": "media/logo/32.png",
            "48": "media/logo/48.png",
            "128": "media/logo/128.png"
        }
    },
    "content_scripts": [{
        "run_at": "document_end",
        "matches": [
            "*://stadia.google.com/u/*",
            "*://stadia.google.com/home*",
            "*://stadia.google.com/store*",
            "*://stadia.google.com/player*",
            "*://stadia.google.com/explore*",
            "*://stadia.google.com/profile*",
            "*://stadia.google.com/settings*",
            "*://stadia.google.com/captures*",
            "*://stadia.google.com/library*"
        ],
        "js": [
            "inject.js",

            "translations/ca.js",
            "translations/da.js",
            "translations/de.js",
            "translations/en.js",
            "translations/eo.js",
            "translations/es.js",
            "translations/fr.js",
            "translations/hu.js",
            "translations/it.js",
            "translations/nl.js",
            "translations/pt.js",
            "translations/ru.js",
            "translations/sk.js",
            "translations/sv.js",

            "translations.js",
            "monitor.js",
            "menu_monitor.js",
            "enhanced.js"
        ]
    }],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [{
        "resources": [
            "injected/stream_info.js",
            "injected/set_resolution.js",
            "injected/log_translations.js",
            "injected/debug_enhanced.js",

            "translations/ca.js",
            "translations/da.js",
            "translations/de.js",
            "translations/en.js",
            "translations/eo.js",
            "translations/es.js",
            "translations/fr.js",
            "translations/hu.js",
            "translations/it.js",
            "translations/nl.js",
            "translations/pt.js",
            "translations/ru.js",
            "translations/sk.js",
            "translations/sv.js",

            "media/included/hunters_logo.png",
            "media/included/hunters_banner.png"
        ],
        "matches": [
            "*://stadia.google.com/*"
        ]
    }],
    "host_permissions": [
        "*://stadia.google.com/*"
    ],
    "key": ""
}