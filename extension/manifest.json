{
    "name": "Stadia Enhanced",
    "description": "Various new features for Google Stadia.",
    "version": "1.18.4",
    "manifest_version": 2,
    "author": "Christopher Klay",
    "homepage_url": "https://github.com/ChristopherKlay/StadiaEnhanced",
    "icons": {
        "16": "media/logo/16.png",
        "32": "media/logo/32.png",
        "48": "media/logo/48.png",
        "128": "media/logo/128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Stadia Enhanced v1.18.4",
        "default_icon": {
            "16": "media/logo/16.png",
            "32": "media/logo/32.png",
            "48": "media/logo/48.png",
            "128": "media/logo/128.png"
        }
    },
    "content_scripts": [{
        "run_at": "document_end",
        "matches": ["https://stadia.google.com/u/*", "*://stadia.google.com/home*", "*://stadia.google.com/store*", "*://stadia.google.com/player*", "*://stadia.google.com/explore*", "*://stadia.google.com/profile*", "*://stadia.google.com/settings*", "*://stadia.google.com/captures*"],
        "js": ["enhanced.js"]
    }],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "web_accessible_resources": ["media/included/hunters_logo.png", "media/included/hunters_banner.png"],
    "permissions": ["tabs", "*://stadia.google.com/*"],
    "key": ""
}